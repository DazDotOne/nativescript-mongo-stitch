<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="pageLoaded" class="page">
  <StackLayout class="p-20">
    <Label text="message" class="t-20 text-center c-black" textWrap="true"/>
    <Button text="Switch to account" tap="{{ switchToAccount }}"/>
    <Button text="Test Insert" tap="{{ testInsert }}"/>
    <Button text="Test Find One Update" tap="{{ testFindOneUpdate }}"/>
    <Button text="Test Local Insert" tap="{{ testLocalInsert }}"/>
    <Button text="Test Local Find" tap="{{ testLocalFind }}"/>
    <Button text="Test Local Find All" tap="{{ testLocalFindAll }}"/>
    <Button text="Test Local Delete All" tap="{{ testLocalDeleteAll }}"/>
  </StackLayout>
</Page>
